
 	<header>
		<div id='mainBtn'>
			<h1>The Republic News Feed</h1>
       <a href="/"><button id="home">Home</button></a>
		</div>
		<div>
			<img id='logo' src='../../assets/image/logo.png' width=50px> 
		</div>
